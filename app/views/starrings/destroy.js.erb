function loadDoc() {
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (xhttp.readyState == 4 && xhttp.status == 200) {
      document.getElementById("unFav").style() = xhttp.responseText;
      StarringUpdate();
    }
  };
  xhttp.open("GET", "starring/calc.rb", true);
  xhttp.send();
}