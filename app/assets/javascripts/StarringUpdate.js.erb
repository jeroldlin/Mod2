function StarringUpdate (){
	if ($("#starringSwitch").text() == "yes"){
		$(".switchNo").css("display", "");
		$(".switchYes").css("display", "none");
	}else{
		$(".switchNo").css("display", "none");
		$(".switchYes").css("display", "");
	}
}

function SwitchFavoritePage (){
	if ($("#starringSwitch").text() == "yes"){
		$("#starringSwitch").text("no");
	}else{
		$("#starringSwitch").text("yes");
	}
}

